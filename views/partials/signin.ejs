<form id="signin" class="modal">
  <div>
    <div class="form-group">
      <div>
        <label for="create-email">メールアドレス</label>
        <input type="text" class="form-control email" name="email" />
      </div>
      <div>
        <label for="create-password">パスワード</label>
        <input
          type="password"
          class="form-control password"
          name="password"
        />
      </div>
    </div>
    <div class="buttons">
      <button type="button" class="btn btn-white reset">
        リセット
      </button>
      <button type="button" class="cancel btn btn-white cancel">
        キャンセル
      </button>
      <button
        type="button"
        id="login"
        data-dismiss="modal"
        class="btn btn-red"
      >
        ログイン
      </button>
    </div>
    <div class="message signin_err"></div>
  </div>
</form>

<script type="module" type=”text/javascript” src="../js/signin.js"></script>